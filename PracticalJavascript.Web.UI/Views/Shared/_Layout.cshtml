@using SquishIt.Framework
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>@ViewBag.Title</title>
  <link href="@Url.Content("~/Content/styles/style.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/styles/960.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/styles/Site.css")" rel="stylesheet" type="text/css" />
</head>
<body>
  <div class="page">
    <header></header>
    <div id="main" class="container_12 shadowbox">      
      @RenderBody()
    </div>
    <footer>
        </footer>
  </div>
  @if (false)
  {
    <script src="@Url.Content("~/Scripts/JSLINQ-vsdoc.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1-vsdoc.js")" type="text/javascript"></script>  
  }
  <script src=" https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript"></script>
  @Html.Partial("_JavascriptGlobals")
    @{  
    var jsBuilder = 
        Bundle.JavaScript()
            .Add("~/Scripts/jquery.tmpl.js")
            .Add("~/Scripts/modernizr-1.7.js")
            .Add("~/Scripts/underscore.js")
            .Add("~/Scripts/underscore.string.js")
            .Add("~/Scripts/debug.min.js")
            .Add("~/Scripts/jquery.cookie.js")
            .Add("~/Scripts/jquery.corner.js")
            .Add("~/Scripts/JSLINQ.js")  
            .Add("~/Scripts/jquery.practical-javascript.js")
            .Add("~/Scripts/practicalJavascript.js")
            .Add("~/Scripts/stores.js")
            .Add("~/Scripts/salesPerson.js");
  }
  @Html.Raw(jsBuilder.Render("~/Scripts/pjs.min.js"))
  <script type="text/javascript">
      $(document).ready(function () {
          pjs.onPageInit();
      });
  </script>
  @RenderSection("Scripts", required: false)
</body>
</html>
